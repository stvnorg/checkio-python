def checkRepeat(s,j):
    if len(s)==1: return 0
    s = s[:len(s)-len(s)%j]
    if len(set([s[i:i+j] for i in range(0,len(s),j)]))==1:
        return s[0:j]
    return 0
    
def repeat(s):
    for i in range(1,len(s)):
        for j in range(len(s)):
            a = s[0:j]
            b = checkRepeat(s[j:],i)
            if b:
                return a+'('+b+')'

if __name__ == '__main__':
    print (repeat('4666666'))
    print (repeat('4166666'))
    print (repeat('3333333333'))
    print (repeat('375'))
    print (repeat('211154545454'))
    print (repeat(''))